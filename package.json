{
  "name": "openai-realtime-agents",
  "version": "0.2.0",
  "private": true,
  "packageManager": "pnpm@9.12.1",
  "engines": {
    "node": "^20.17.0"
  },
  "scripts": {
    "dev": "pnpm --filter @openai-realtime-agents/web dev",
    "build": "pnpm --filter @openai-realtime-agents/web build",
    "start": "pnpm --filter @openai-realtime-agents/web start",
    "lint": "pnpm --filter @openai-realtime-agents/web lint",
    "infra:dev": "sst dev",
    "infra:deploy": "sst deploy",
    "infra:remove": "sst remove",
    "infra:outputs": "sst outputs",
    "infra:deploy:dev": "pnpm infra:deploy -- --stage dev",
    "infra:deploy:stg": "pnpm infra:deploy -- --stage stg",
    "infra:deploy:prod": "pnpm infra:deploy -- --stage prod",
    "infra:destroy:dev": "pnpm infra:remove -- --stage dev",
    "infra:destroy:stg": "pnpm infra:remove -- --stage stg",
    "infra:destroy:prod": "pnpm infra:remove -- --stage prod",
    "lint:infra": "eslint --max-warnings=0 --ext .ts,.tsx sst.config.ts infra packages/shared",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "sst": "^3.1.44"
  }
}
